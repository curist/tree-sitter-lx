{
  "grammars": [
    {
      "name": "lx",
      "camelcase": "Lx",
      "scope": "source.lx",
      "path": ".",
      "file-types": [
        "lx"
      ],
      "highlights": "queries/highlights.scm",
      "injection-regex": "^lx$"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Lx grammer for tree-sitter",
    "authors": [
      {
        "name": "curist"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-lx"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
